{
  "name": "fatcoupon-cleaner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.js",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node src/server.js",
    "server": "NODE_ENV=production pm2 stop all && pm2 start src/server.js"
  },
  "dependencies": {
    "antd": "^4.15.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "bull": "^3.22.0",
    "ioredis": "^4.26.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-router": "^10.0.0",
    "koa-session": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "next": "10.1.2",
    "postcss": "^8.2.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.12",
    "swr": "^0.5.5",
    "tailwindcss": "^2.1.1",
    "xvfb": "^0.4.0"
  },
  "nodemonConfig": {
    "watch": [
      "src/app/"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js",
    "ignore": [
      ".git/",
      ".next/",
      "node_modules/",
      "src/app/chrome/dist/",
      "src/app/chrome/mappings/",
      "src/app/chrome/cookies/"
    ]
  }
}
